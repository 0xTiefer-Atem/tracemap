<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.trace.map.mapper.TraceInfoMapper">
    <resultMap id="BaseResultMap" type="org.trace.map.entity.TraceInfo">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="trace_id" jdbcType="VARCHAR" property="traceId"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
    </resultMap>

    <select id="getTraceInfoList" resultType="org.trace.map.entity.TraceInfo">
        select `trace_id`, `create_time` from trace_map
    </select>

</mapper>